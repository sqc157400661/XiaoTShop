@import "../../lib/wxParse/wxParse.wxss";


page {
    background-color: #fff;
}

.container {
    min-height: 100%;
    padding-bottom: 100rpx;
}

.swiper-container {
    width: 100%;
    position: relative;
    background-color: #f9f9f9;
}

.swiper_box {
    width: 100%;
    height: 750rpx;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
    height: 750rpx;
}

.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    display: flex;
    justify-content: center;
}

.swiper-container .dots .dot {
    margin: 0 8rpx;
    width: 20rpx;
    height: 8rpx;
    background: #fff;
    border-radius: 2rpx;
    transition: all 0.6s;
    opacity: 0.5;
}

.swiper-container .dots .dot.active {
    opacity: 0.9;
}

.goods-info {
    background-color: #fff;
    padding-top: 15px;
    width: 100%;
    position: relative;
    padding-bottom: 5px;
    border-bottom: 15px solid #f5f5f5;
}

.row-arrow {
    background-color: #fff;
    width: 100%;
    font-size: 26rpx;
    height: 45px;
    line-height: 45px;
    color: #333;
    border-bottom: 15px solid #f5f5f5;
}

.row-arrow image {
    width: 10px;
    height: 21px;
    float: right;
    margin-right: 15px;
    margin-top: 12px;
}

.goods-left {
    width: 100%;
}

.goods-title {
    font-size: 34rpx;
    color: #333;
    padding-left: 15px;
}

.goods-characteristic {
    font-size: 28rpx;
    color: #999;
    padding-left: 15px;
    padding-top: 5px;
    padding-bottom: 5px;
}

.goods-price {
    padding-left: 15px;
    color: #b5272d;
    font-size: 40rpx;
    padding-top: 5px;
    display: inline-block;
}

.goods-price text {
    font-size: 24rpx;
    padding-right: 3px;
}

.goods-originalPrice {
    display: inline-block;
    padding-left: 10px;
    font-size: 30rpx;
    color: #999;
    line-height: 26px;
}

.goods-originalPrice {
    font-size: 24rpx;
    padding-right: 3px;
}

.piangjia {
    float: right;
    padding-right: 15px;
    color: #999;
    font-size: 26rpx;
    line-height: 40px;
}

.goods-des-info {
    background-color: #fff;
    width: 100%;
    margin-bottom: 15px;
    padding-bottom: 15px;
}

.label-title {
    font-size: 28rpx;
    text-align: center;
    padding-top: 10px;
    padding-bottom: 10px;
    color: #333;
    border-bottom: 1px solid #f5f5f5;
    margin-bottom: 15px;
}

.goods-text {
    font-size: 28rpx;
    color: #333;
    line-height: 25px;
}

.goods-text {
    padding-left: 5px;
}

.des-imgs {
    width: 100%;
}

.des-imgs image {
    width: 100%;
}

.goods-home {
    position: fixed;
    bottom: 70px;
    right: 20px;
    background-color: #fff;
    width: 40px;
    height: 40px;
    text-align: center;
    border-radius: 50%;
    border: 1px solid #fff;
    box-shadow: 0px 1px 6px -1px #666;
    line-height: 40px;
}

.kanjia {
    position: fixed;
    bottom: 120px;
    right: 20px;
    background-color: #fff;
    width: 40px;
    height: 40px;
    text-align: center;
    border-radius: 50%;
    border: 1px solid #fff;
    box-shadow: 0px 1px 6px -1px #666;
    line-height: 40px;
}

.kanjia image {
    width: 30px;
    height: 30px;
    float: left;
    padding-left: 5px;
    padding-top: 3px;
}

.goods-home image {
    width: 28px;
    height: 28px;
    background-color: #fff;
    float: left;
    padding-left: 7px;
    padding-top: 5px;
}

.footer-box {
    width: 100%;
    height: 45px;
    background-color: #b5272d;
    position: fixed;
    bottom: 0;
    left: 0;
    box-shadow: 0rpx 15rpx 15rpx 10rpx;
    text-align: center;
    line-height: 45px;
    font-size: 28rpx;
    color: #fff;
    font-weight: 300;
}

.box-list {
    width: 14%;
    border-right: 1px solid #f2f2f2;
    position: relative;
}

button.box-list {
    padding-left: 0;
    border-radius: 0px;
    border-right: 1px solid #f2f2f2;
    background-color: #fff;
}

button.box-list::after {
    border-radius: 0px;
    border: 0px;
}

.box-list image {
    width: 25px;
    height: 25px;
    padding-top: 16px;
    padding-left: 15px;
}

.shop-num {
    font-size: 22rpx;
    background-color: #b5272d;
    color: #fff;
    width: 18px;
    height: 18px;
    line-height: 17px;
    text-align: center;
    position: absolute;
    top: 10px;
    right: 10px;
    border-radius: 10px;
}

.box-title {
    width: 29%;
    font-size: 32rpx;
    line-height: 59px;
    text-align: center;
}

.buy-now {
    background-color: #b5272d;
    color: #fff;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 4;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 5;
}

.popup-contents {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 6;
}

.pop-goods-info {
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-left: 30rpx;
    padding: 30rpx 0;
    border-bottom: 1px solid #f5f5f5;
}

.pop-img-box {
    width: 120rpx;
    height: 120rpx;
    overflow: hidden;
    margin-right: 26rpx;
}

.pop-img-box .goods-thumbnail {
    width: 120rpx;
    height: 120rpx;
}

.pop-goods-title {
    width: 484rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 26rpx;
    color: #000;
}

.pop-goods-price {
    font-size: 26rpx;
    color: #b5272d;
    margin-top: 20rpx;
}

.pop-goods-close {
    width: 36rpx;
    height: 36rpx;
    flex: 1;
    background: url("https://cdn.it120.cc/images/weappshop/popup-close.png") no-repeat center center;
    background-size: 36rpx auto;
    align-self: flex-start;
    margin-top: -10rpx;
}

.size-label-box .label {
    font-size: 26rpx;
    color: #000;
    padding-left: 30rpx;
    padding: 30rpx 0 20rpx 30rpx;
}

.size-label-box .label-item-box {
    display: flex;
    margin-left: 30rpx;
    flex-direction: row;
    flex-wrap: wrap;
}

.size-label-box .label-item {
    font-size: 26rpx;
    color: #000;
    padding: 14rpx 20rpx;
    border: 1px solid #ddd;
    border-radius: 6rpx;
    margin: 0 20rpx 20rpx 0;
}

.size-label-box .label-item.active {
    color: #b5272d;
    border: 1px solid #b5272d;
}

.buy-num-box {
    display: flex;
    justify-content: space-between;
    padding: 30rpx 30rpx 48rpx 0;
    margin-left: 30rpx;
    border-top: 1px solid #f5f5f5;
    margin-top: 30rpx;
    align-items: center;
}

.num-label {
    font-size: 26rpx;
    color: #000;
}

.buy-num-box .num-box {
    display: flex;
}

.buy-num-box .num-box .num-jian,.buy-num-box .num-box .num-input,.buy-num-box .num-box .num-jia {
    width: 80rpx;
    height: 64rpx;
    line-height: 62rpx;
    text-align: center;
    border: 1px solid #eee;
}

.buy-num-box .num-box .num-input {
    font-size: 28rpx;
}

.buy-num-box .num-box .num-input input {
    height: 100%;
}

.popup-join-btn {
    width: 100%;
    height: 89rpx;
    text-align: center;
    line-height: 89rpx;
    font-size: 34rpx;
    color: #fff;
    background-color: #b5272d;
}

.buy-num-box .num-box .hui {
    background-color: #f5f5f9;
}

.goods-reputa-list {
    width: 100%;
    height: 90px;
    border-bottom: 1px dashed #f5f5f5;
    margin-bottom: 15px;
}

.goods-reputa-avatar {
    float: left;
    width: 15%;
    height: 90px;
    padding-left: 15px;
}

.goods-reputa-avatar .goods-avatar {
    width: 50px;
    height: 50px;
    border-radius: 50%;
}

.goods-reputa-mark {
    float: left;
    width: 75%;
}

.goods-reputa-user {
    font-size: 28rpx;
    color: #333;
}

.goods-reputa-user text {
    background-color: #b5272d;
    color: #fff;
    font-size: 24rpx;
    margin-left: 10px;
    padding: 0 5px;
    border-radius: 8rpx;
}

.goods-mark {
    font-size: 28rpx;
    color: #333;
    padding-top: 8px;
    padding-left: 5px;
}

.goods-mark-day {
    font-size: 22rpx;
    color: #999;
    margin-top: 10px;
}

.goods-mark-day text {
    padding-left: 15px;
}

.tab-header {
    width: 100%;
    padding-top: 10px;
    background-color: #fff;
    margin: 0 auto;
    margin-bottom: 10px;
}

.tab-header view {
    display: inline-block;
    width: 50%;
    text-align: center;
    font-size: 30rpx;
    color: #999;
    height: 30px;
    padding-bottom: 5px;
    border-bottom: 1px solid #f5f5f5;
}

.tab-header .active {
    color: #b4282d;
    border-bottom: 2rpx solid #b4282d;
}

.tab-container {
    width: 100%;
    margin-bottom: 20px;
}

.tab-container .tab-item {
    width: 100%;
    display: none;
}

.tab-container .tab-item.active {
    display: block;
}

.reputation-none {
    width: 100%;
    height: 300px;
    color: #999;
    text-align: center;
    font-size: 24rpx;
    margin-top: 15px;
}

.reputation-none image {
    display: block;
    margin: 0 auto;
    width: 110px;
    height: 110px;
    margin-bottom: 5px;
}

.tab-item-list {
    background-color: #fff;
    margin-top: 10px;
    padding: 10px;
    height: 100px;
}

.tab-item-list image {
    width: 100px;
    height: 100px;
    float: left;
    border-radius: 6rpx;
}

.tab-item-list-info {
    width: 70%;
    padding-left: 10px;
    float: left;
    font-size: 30rpx;
    color: #333;
}

.show-container {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.3);
    z-index: 99999999999;
}

.show-container .show-box {
    width: 70%;
    margin: 0 auto;
    height: 600rpx;
    background-color: #fff;
    margin-top: 40%;
    border-radius: 10rpx;
}

.show-logo-box {
    text-align: center;
    width: 80%;
    border-bottom: 1px solid #f5f5f5;
    margin: 0 auto;
    padding-top: 20px;
}

.show-logo-box image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    display: block;
    margin: 0 auto;
}

.show-mall-name {
    font-size: 32rpx;
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    color: #333;
}

.show-text-box {
    margin-top: 20rpx;
    margin-bottom: 40rpx;
}

.show-text-name {
    font-size: 30rpx;
    color: #333;
    width: 80%;
    margin: 0 auto;
}

.show-text-info {
    width: 80%;
    margin: 0 auto;
    font-size: 24rpx;
    color: #999;
    margin-top: 10rpx;
}

.show-text-info text {
    margin-right: 10rpx;
    font-size: 26rpx;
}

.show-box button::after {
    border-radius: 4rpx;
    border: 0;
}

.show-box button {
    width: 80%;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    background-color: #04be01;
    color: #fff;
}

.goods-button {
    position: fixed;
    bottom: 200rpx;
    right: 30rpx;
    width: 90rpx;
    height: 90rpx;
}

.goods-button image {
    width: 70rpx;
    height: 70rpx;
    box-shadow: 0 0 20rpx #eee;
    border-radius: 50%;
    display: block;
    margin: 0 auto;
    margin-top: 10rpx;
    position: relative;
}

.goods-button image button {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}